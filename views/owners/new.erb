<form method="post" action="/owners">
  <label for="first-name-field">First name:</label>
  <input type="text" name="first_name" id="first-name-field"/>

  <label for="last-name-field">Last name:</label>
  <input type="text" name="last_name" id="last-name-field"/>

  <label for="address-field">Address:</label>
  <input type="text" name="address" id="address-field"/>

  <label for="email-field">Email:</label>
  <input type="text" name="email" id="email-field"/>

  <label for="photo-url-field">Photo:</label>
  <input type="text" name="photo_url" id="photo-url-field"/>

  <label for="phone-number-field">Phone:</label>
  <input type="text" name="phone_number" id="phone-number-field"/>

  <input type="submit" value="Add new owner to list">
</form>
